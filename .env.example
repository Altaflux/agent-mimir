

# OPENAI API KEY
OPENAI_API_KEY=

# Discord bot token
#DISCORD_TOKEN=

# Enable the code interpreter plugin.
CODE_INTERPRETER_PLUGIN=true

# Enable the web browser plugin.
WEB_BROWSER_PLUGIN=true

# Set the model to use for the AI agent.
#AGENT_OPENAI_MODEL=gpt-4-turbo

# Log tool response
#MIMIR_LOG_TOOL_RESPONSE=true

# Log AI response
#MIMIR_LOG_AI_RESPONSE=true

# Define a persistent working directory for the AI agents to persist there state.
#WORK_DIRECTORY=

# API server host and port.
#MIMIR_API_HOST=0.0.0.0
#MIMIR_API_PORT=8787

# API server route prefix (used by agent-mimir-api-server).
#MIMIR_API_PREFIX=/v1

# Public base path used in generated download href values.
# Set to /api when using web proxies, or /v1 for direct API access.
#MIMIR_PUBLIC_API_BASE_PATH=/api

# Service-to-service token between web proxies and API server.
#MIMIR_API_SERVICE_TOKEN=

# Base URL used by web server proxies to reach the API server.
#MIMIR_API_BASE_URL=http://127.0.0.1:8787

